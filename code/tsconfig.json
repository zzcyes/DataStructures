{
    "compilerOptions": {
        "module": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "es2015",
            "DOM"
        ],
        "outDir": "./dist/",
        "target": "es5",
        "allowJs": true
    }
}
